msgstr ""

msgid ""
"--list-repositories\t\tList all repositories in this version, example: --"
"list-repositories trusty"
msgstr ""

msgid ""
"--description-repository  \tView repository description, example: --"
"description-repository trusty uniaolivre"
msgstr ""

msgid ""
"--description-all-repositories\tView all repositories descriptions for one "
"version, example: --description-all-repositories trusty"
msgstr ""

msgid ""
"--add-repository\t\tAdd repository in remaster, example: --add-repository "
"trusty uniaolivre \\\"/home/user/remaster\\\""
msgstr ""

msgid ""
"--add-all-repositories\t\tAdd repositories in remaster, example: --add-all-"
"repositories trusty \\\"/home/user/remaster\\\""
